"))
  (:|listitem| NIL (:|prepend| NIL "•") "
"
   (:|para| NIL (:|code| NIL ">") " is converted to " (:|code| NIL "&gt;") "
"))
  (:|listitem| NIL (:|prepend| NIL "•") "
"
   (:|para| NIL (:|code| NIL "'") " (single quote) is converted to "
    (:|code| NIL "&#39;") "
"))
  (:|listitem| NIL (:|prepend| NIL "•") "
"
   (:|para| NIL (:|code| NIL "\"") " (double quote) is converted to "
    (:|code| NIL "&quot;") "
"))
  (:|listitem| NIL (:|prepend| NIL "•") "
"
